{
  "name": "learn-graphql",
  "version": "1.0.0",
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "vite serve",
    "start": "run-p \"type-check --watch\" \"dev\"",
    "build": "vite build",
    "codegen": "graphql-codegen --config ./codegen.yaml"
  },
  "dependencies": {
    "graphql": "16.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "urql": "4.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/client-preset": "3.0.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "npm-run-all": "4.1.5",
    "typescript": "5.0.3",
    "vite": "4.2.1"
  }
}
